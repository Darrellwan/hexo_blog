{
    "rewrites": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "header",
            "key": "accept",
            "value": "(.*)text/markdown(.*)"
          }
        ],
        "destination": "/:path*/index.md"
      }
    ],
    "headers": [
      {
        "source": "/(.*_bg.*)\\.(jpg|jpeg|png|gif|webp|svg)$",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self' 'unsafe-inline' 'unsafe-eval' *; img-src 'self' data: *;"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "Referrer-Policy",
            "value": "no-referrer-when-downgrade"
          },
          {
            "key": "Permissions-Policy",
            "value": "geolocation=(self), browsing-topics=()"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/n8n-resources",
        "destination": "/n8n-tutorial-resources",
        "permanent": true
      }
    ]
}
